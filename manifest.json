{
  "manifest_version": 3,
  "name": "UI Injector Extension",
  "version": "1.0",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.js", "styles.css"],
      "matches": ["<all_urls>"]
    }
  ]
}